<IfModule mod_rewrite.c>
    RewriteEngine On
    
    # Handle CORS preflight requests
    RewriteCond %{REQUEST_METHOD} OPTIONS
    RewriteRule ^(.*)$ $1 [R=200,L]
</IfModule>

# Set PHP to handle JSON properly
<IfModule mod_php.c>
    php_flag display_errors Off
    php_flag log_errors On
</IfModule>
